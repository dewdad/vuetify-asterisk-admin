{
  "name": "vuetify-asterisk-admin",
  "version": "1.0.0",
  "description": "vuetify-asterisk-admin",
  "author": "dani.lesmiadi@gmail.com",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test:unit": "TEST=unit yarn ava ./test/specs/*.js --verbose",
    "test:e2e": "TEST=e2e yarn ava ./test/e2e/*.js --verbose",
    "test": "yarn test:unit & yarn test:e2e",
    "prod": "pm2 start npm ecosystem.config.js --name dashboard -- start"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/setup.js"
    ],
    "files": [
      "./test/specs/*.js"
    ],
    "snapshotDir": "./test/snapshots"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.1",
    "@nuxtjs/axios": "^5.3.1",
    "animate.css": "^3.6.1",
    "axios": "^0.18.0",
    "change-case": "^3.0.2",
    "cookieparser": "^0.1.0",
    "export-to-csv": "^0.1.2",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.1",
    "node-sass": "^4.9.2",
    "noty": "^3.2.0-beta",
    "nuxt": "^1.0.0",
    "sass-loader": "^7.0.2",
    "vee-validate": "^2.1.0-beta.8",
    "vue-moment": "^4.0.0",
    "vue-socket.io": "^2.1.1-b",
    "vue2-google-maps": "^0.9.7",
    "vuetify": "^1.2.3"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-env": "^3.2.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jsdom": "^11.12.0",
    "prettier": "^1.12.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
